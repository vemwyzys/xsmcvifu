.tipsMask
{
    display:none;
    position:fixed;
    width:100%;
    height:100%;
    background-color:rgba(0,0,0,0.6);
    z-index:10;
    top:0;
}
.tips_content
{
    width:400px;
    border:none;
    margin:0 auto;
    background-color:white;
    margin-top:100px;
    border-radius: 10px;
    font-size: 14px;
}
.tipsMaskText
{
    padding: 30px 15px;
    text-align: center;
    color: #333333;
    font-size: 18px;
}
.ButtonTwo
{
    text-align: center;
}
.tipsYes
{
     width: 100px;
    height: 40px;
    text-align: center;
    margin-bottom: 15px;
    font-size: 16px;
    font-family: "Microsoft Yahei";
    border: none;
    cursor: pointer;
    background-color: #f05634;
    color: white;
}
.tipsYes:hover
{
    background-color: #df3b17;
}