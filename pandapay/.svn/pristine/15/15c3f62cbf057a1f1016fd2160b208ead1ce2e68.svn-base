package com.pandapay.entity.DO;

import java.sql.Timestamp;

/**
 * 用户账户金额记录
 * @author gql
 *
 */
public class UserRecordAmountDO {
	
	private long id; //记录Id
	private int userId; //用户Id
	private int inOutType; //收入支出类型，1：收入，2：支出
	private double amount; //收支金额，单位：元
	private String serviceType; //业务类型
	private String remarks; //备注
	private Timestamp addTime; //记录时间
	
	/**
	 * 记录Id
	 * @return the id
	 */
	public long getId() {
		return id;
	}
	
	/**
	 * 记录Id
	 * @param id the id to set
	 */
	public void setId(long id) {
		this.id = id;
	}
	
	/**
	 * 用户Id
	 * @return the userId
	 */
	public int getUserId() {
		return userId;
	}
	
	/**
	 * 用户Id
	 * @param userId the userId to set
	 */
	public void setUserId(int userId) {
		this.userId = userId;
	}
	
	/**
	 * 收入支出类型，1：收入，2：支出
	 * @return the inOutType
	 */
	public int getInOutType() {
		return inOutType;
	}
	
	/**
	 * 收入支出类型，1：收入，2：支出
	 * @param inOutType the inOutType to set
	 */
	public void setInOutType(int inOutType) {
		this.inOutType = inOutType;
	}
	
	/**
	 * 收支金额，单位：元
	 * @return the amount
	 */
	public double getAmount() {
		return amount;
	}
	
	/**
	 * 收支金额，单位：元
	 * @param amount the amount to set
	 */
	public void setAmount(double amount) {
		this.amount = amount;
	}
	
	/**
	 * 业务类型
	 * @return the serviceType
	 */
	public String getServiceType() {
		return serviceType;
	}
	
	/**
	 * 业务类型
	 * @param serviceType the serviceType to set
	 */
	public void setServiceType(String serviceType) {
		this.serviceType = serviceType;
	}
	
	/**
	 * 备注
	 * @return the remarks
	 */
	public String getRemarks() {
		return remarks;
	}
	
	/**
	 * 备注
	 * @param remarks the remarks to set
	 */
	public void setRemarks(String remarks) {
		this.remarks = remarks;
	}
	
	/**
	 * 记录时间
	 * @return the addTime
	 */
	public Timestamp getAddTime() {
		return addTime;
	}
	
	/**
	 * 记录时间
	 * @param addTime the addTime to set
	 */
	public void setAddTime(Timestamp addTime) {
		this.addTime = addTime;
	}
	
}
