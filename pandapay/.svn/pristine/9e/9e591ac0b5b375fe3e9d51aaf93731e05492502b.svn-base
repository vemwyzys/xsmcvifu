.condition
{
    display: inline-block;
    margin-left: 15px;
    margin-bottom: 15px;
}
.query
{
    width: 140px;
    height: 35px;
    border-radius: 5px;
    border: 1px solid #dddddd;
    padding-left: 4px;
}
.startTime,.endTime
{
    width: 156px;
    height: 35px;
    border-radius: 5px;
    border: 1px solid #dddddd;
    padding-left: 4px;
    margin-left: 4px;
}
.choose
{
    width: 120px;
    height: 35px;
    border-radius: 5px;
    border: 1px solid #dddddd;
}
.ask
{
    margin-left: 15px;
    width: 80px;
    height: 35px;
    border: none;
    font-size: 16px;
    text-align: center;
    font-family: "Microsoft Yahei";
    vertical-align: middle;
    border-radius: 5px;
    background-color: #febc5e;
    cursor: pointer;
    color: white;
}
.ask:hover
{
    background-color: #ffa92d;
}
.educe
{
    margin-left: 15px;
    width: 80px;
    height: 35px;
    border: none;
    font-size: 16px;
    text-align: center;
    font-family: "Microsoft Yahei";
    vertical-align: middle;
    border-radius: 5px;
    background-color: #5bb559;
    cursor: pointer;
    color: white;
}
.educe:hover
{
    background-color: #2aa32a
}
.order
{
    width: 215px;
}
.account
{
    width: 155px;
}
.money
{
    width: 120px;
}
.business
{
    width: 100px;
}
.pay
{
    width: 84px;
}
.time
{
    width: 145px;
}
.state
{
    width: 80px;
}
.mark
{
    width: 219px;
}