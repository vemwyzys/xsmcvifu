.top
{
    width: 100%;
    height: 210px;
}
.top .swiper-container
{
    margin: 0 auto;
    width: 100%;
    height: 210px;
    float: left;
}
.top .swiper-wrapper
{
    width: 100%;
    height: 210px;
    line-height: 210px;
}
.top .swiper-slide
{
    width: 100%;
    height: 210px;
    float: left;
    overflow: hidden;
}
.table
{
    margin:10px auto;
    width: 100%;
    background-color: #ffffff;
    border-collapse:collapse;
}
.tableInfo:last-of-type
{
    border-top: 1px solid #ececec;
}
.details
{
    padding: 15px 10px;
    width: 33%;
    text-align: center;
    border-right: 1px solid #ececec;
}
.details:nth-child(3n)
{
    border-right:none;
}
.tableTitle
{
    display: inline-block;
    width: 100%;
    color: #333333;
    font-size: 14px;
}
.img
{
    width: 60px;
}
.logoImg
{
    width: 60px;
}
.footer
{
    margin:0 auto;
    width: 90%;
    height:150px;
}
.footer .swiper-container
{
    margin: 0 auto;
    width: 100%;
    height: 150px;
    float: left;
}
.footer .swiper-wrapper
{
    width: 100%;
    height: 150px;
    line-height: 210px;
}
\footer .swiper-slide
{
    width: 100%;
    height: 150px;
    float: left;
    overflow: hidden;
}