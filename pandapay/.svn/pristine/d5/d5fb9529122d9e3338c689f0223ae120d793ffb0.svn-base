.condition
{
    display: inline-block;
    margin-left: 15px;
    margin-bottom: 15px;
}
.query
{
    width: 150px;
    height: 35px;
    border-radius: 5px;
    border: 1px solid #dddddd;
    padding-left: 4px;
}
.startTime,.endTime
{
    width: 186px;
    height: 35px;
    border-radius: 5px;
    border: 1px solid #dddddd;
    padding-left: 4px;
    margin-left: 4px;
}
.choose
{
    width: 130px;
    height: 35px;
    border-radius: 5px;
    border: 1px solid #dddddd;
}
.ask
{
    margin-left: 15px;
    width: 80px;
    height: 35px;
    border: none;
    font-size: 16px;
    text-align: center;
    font-family: "Microsoft Yahei";
    vertical-align: middle;
    border-radius: 5px;
    background-color: #febc5e;
    cursor: pointer;
    color: white;
}
.ask:hover
{
    background-color: #ffa92d;
}
.time
{
    width: 150px;
}
.appVersion,.appNumber
{
    width: 100px;
}
.appState,.appLink
{
    width: 361px;
}
.appLink a
{
    color: #f05634;
}
.appLink a:hover
{
    color: #df3b17;
    text-decoration: underline;
}
.operate
{
    width: 60px;
}
.delete
{
    text-align: center;
    color: #f05634;
    font-size: 14px;
    font-family: "Microsoft YaHei";
    cursor: pointer;
    border-radius: 5px;
    margin-right: 3px;
    width: 45px;
    border: none;
    background-color: transparent;
}
.delete:hover
{
    color: #df3b17;
}

.add_pop,.delete_pop
{
    display:none;
    z-index:999;
}
.mask_tips
{
    padding: 20px 15px;
    text-align: left;
    color: #666666;
    font-size: 16px;
    word-break: break-all;
}
.entry_tips
{
    padding: 10px 0 0 0;
    text-align: left;
    color: #666666;
    font-size: 14px;
    position: relative;
}
.sign
{
    width: 80px;
    margin-left: 15px;
    display: inline-block;
}
.sign span
{
    color: red;
}
.file
{
    opacity: 0;
    position: absolute;
    width: 80px;
    height: 37px;
    left: 302px;
    cursor: pointer;
}
.choose
{
    width: 190px;
    height: 35px;
    padding-left: 4px;
    border:1px solid #cccccc;
    border-radius: 5px;
}
.choose_button
{
    width: 80px;
    height: 35px;
    margin-left: 2px;
    background-color: #f05634;
    cursor: pointer;
    text-align: center;
    color: white;
    font-size: 14px;
    font-family: "Microsoft Yahei";
    border: 1px solid #f05634;
}
.choose_button:hover
{
    background-color: #df3b17;
}
.box
{
    width: 280px !important;
}
.choice
{
    width: 286px !important;
}
.txt
{
    vertical-align: top;
    padding: 4px;
    width: 276px;
    height: 60px;
    border-radius: 5px;
    border: 1px solid #cccccc;
    resize: none;
}
.Button
{
    margin-top: 15px;
}
