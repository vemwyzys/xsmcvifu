package com.pandapay.entity.BO;

/**
 * 数值改变 数据模型
 * @author whx
 *
 */
public class ChangeNumberBO {

	private long changeObject;  //改变对象标记
	private double changeNumber;  //改变的数值
	
	/**
	 * 改变对象标记
	 * @return the changeObject
	 */
	public long getChangeObject() {
		return changeObject;
	}
	
	/**
	 * 改变对象标记
	 * @param changeObject the changeObject to set
	 */
	public void setChangeObject(long changeObject) {
		this.changeObject = changeObject;
	}
	
	/**
	 * 改变的数值
	 * @return the changeNumber
	 */
	public double getChangeNumber() {
		return changeNumber;
	}
	
	/**
	 * 改变的数值
	 * @param changeNumber the changeNumber to set
	 */
	public void setChangeNumber(double changeNumber) {
		this.changeNumber = changeNumber;
	}
	
}
